{% extends "NAOCoreBundle::layout.html.twig" %}

{% block banner_h2 %}Titre de la page register{% endblock %}
{% block banner_desc %}Description de la page register{% endblock %}

{% block center_column %}
	<div class="well">
		{{ form_start(form) }}
			{{ form_errors(form) }}
			{{ form_row(form.username) }}
			{{ form_row(form.lastname) }}
			{{ form_row(form.firstname) }}
			{{ form_row(form.accountType) }}
			{{ form_row(form.password) }}
			{{ form_row(form.question) }}
			<div class="form-group">
				<div class="col-sm-2"></div>
				<div class="col-sm-10">{{ form_widget(form.validate) }}</div>
			</div>
			{{ form_rest(form) }}
		{{ form_end(form) }}
	</div>
{% endblock %}
